         id="org.eclipse.mylyn.wikitext.context.ui"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

   <plugin
         id="org.eclipse.mylyn.wikitext.asciidoc.core"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

